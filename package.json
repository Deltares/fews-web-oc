    "@deltares/fews-wms-requests": "^1.0.2",
    "@indoorequal/vue-maplibre-gl": "^7.5.1",
    "@jsonforms/core": "^3.4.1",
    "@jsonforms/vue": "^3.4.1",
    "@jsonforms/vue-vuetify": "^3.4.1",
    "@turf/bbox": "^7.1.0",
    "@turf/boolean-intersects": "^7.1.0",
