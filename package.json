{
  "name": "fews-weboc",
  "private": true,
  "version": "0.6.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:deltares": "vue-tsc && env-cmd -f deltares.env vite build",
    "build:weboc": "vue-tsc && vite build --base=/weboc/",
    "preview": "vite preview",
    "test": "vitest run",
    "lint": "npm run prettier",
    "prettier": "prettier . --check",
    "format": "prettier .  --write",
    "sonar": "sonar-scanner -Dsonar.host.url=$SONAR_URL -Dsonar.login=$SONAR_TOKEN -Dsonar.projectKey=$SONAR_KEY -Dsonar.projectName='Delft-FEWS Web OC'"
  },
  "dependencies": {
    "@deltares/fews-pi-requests": "^1.0.0-rc.1",
    "@deltares/fews-ssd-requests": "^1.0.0-rc.1",
    "@deltares/fews-ssd-webcomponent": "^1.0.0-rc.1",
    "@deltares/fews-web-oc-charts": "^3.0.0-alpha.19",
    "@deltares/fews-wms-requests": "^0.1.11-alpha.4",
    "@studiometa/vue-mapbox-gl": "^2.2.0",
    "@turf/helpers": "^7.0.0-alpha.2",
    "@turf/projection": "^7.0.0-alpha.2",
    "@vueuse/core": "^10.5.0",
    "crypto-js": "^4.2.0",
    "d3": "^7.8.5",
    "lodash-es": "^4.17.21",
    "luxon": "^3.4.3",
    "mapbox-gl": "^2.15.0",
    "oidc-client-ts": "^2.4.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "typescript-json-serializer": "^6.0.1",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5",
    "vue-slider-component": "^4.1.0-beta.7",
    "vuetify": "^3.4.10"
  },
  "devDependencies": {
    "@mdi/font": "^7.3.67",
    "@types/crypto-js": "^4.1.3",
    "@types/d3": "^7.4.2",
    "@types/lodash-es": "^4.17.10",
    "@types/luxon": "^3.3.3",
    "@types/mapbox-gl": "^2.7.17",
    "@types/node": "^20.8.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-typescript": "^12.0.0",
    "csstype": "^3.1.2",
    "env-cmd": "^10.1.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.18.1",
    "geojson": "^0.5.0",
    "jsdom": "^23.2.0",
    "prettier": "3.0.3",
    "sonarqube-scanner": "^3.1.0",
    "typescript": "^5.2.2",
    "vite-plugin-vuetify": "^2.0.1",
    "vite": "^5.0.12",
    "vitest": "^1.2.0",
    "vue-tsc": "^1.8.22"
  },
  "volta": {
    "node": "20.5.1"
  }
}
