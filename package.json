{
  "name": "fews-weboc",
  "private": true,
  "version": "1.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:deltares": "vue-tsc && env-cmd -f deltares.env vite build",
    "build:weboc": "vue-tsc && vite build --base=/weboc/",
    "preview": "vite preview",
    "test": "vitest run",
    "test:dev": "vitest",
    "lint": "npm run prettier",
    "prettier": "prettier . --check",
    "format": "prettier .  --write",
    "sonar": "sonar-scanner -Dsonar.host.url=$SONAR_URL -Dsonar.login=$SONAR_TOKEN -Dsonar.projectKey=$SONAR_KEY -Dsonar.projectName='Delft-FEWS Web OC'",
    "test-ct": "playwright test -c playwright-ct.config.ts"
  },
  "dependencies": {
    "@deltares/fews-pi-requests": "^1.12.5",
    "@deltares/fews-ssd-requests": "^1.1.1",
    "@deltares/fews-ssd-webcomponent": "^1.1.1",
    "@deltares/fews-web-oc-charts": "^4.0.0-alpha.0",
    "@deltares/fews-wms-requests": "^2.0.0",
    "@deltares/webgl-streamline-visualizer": "^2.2.0",
    "@indoorequal/vue-maplibre-gl": "^8.3.0",
    "@jsonforms/core": "^3.5.1",
    "@jsonforms/vue": "^3.5.1",
    "@jsonforms/vue-vuetify": "^3.5.1",
    "@turf/bbox": "^7.2.0",
    "@turf/boolean-intersects": "^7.2.0",
    "@turf/circle": "^7.2.0",
    "@turf/helpers": "^7.2.0",
    "@turf/projection": "^7.2.0",
    "@turf/turf": "^7.2.0",
    "@vueuse/core": "^13.0.0",
    "crypto-js": "^4.2.0",
    "d3": "^7.9.0",
    "geotiff": "^2.1.3",
    "lodash-es": "^4.17.21",
    "luxon": "^3.5.0",
    "maplibre-gl": "^5.2.0",
    "oidc-client-ts": "^3.2.0",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "terra-draw": "^1.3.0",
    "terra-draw-maplibre-gl-adapter": "^1.0.2",
    "typescript-json-serializer": "^6.0.1",
    "vue": "^3.5.13",
